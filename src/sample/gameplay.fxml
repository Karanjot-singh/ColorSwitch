<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.Group?>

<VBox prefHeight="635.0" prefWidth="635.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
	  fx:controller="sample.GameplayController" alignment="CENTER" spacing="20" stylesheets="@generalStyle.css, @gameplayStyle.css">

	<Label text="Lets play a game" fx:id="gameMsg" onMouseMoved="#msgHover"/>
	<Group fx:id="arcGrp">
	<Arc fx:id="arc3" centerX="150.0" centerY="150.0" fill="#1f93ff00" length="85.0" radiusX="100.0" radiusY="100.0" startAngle="270.0" stroke="#feff00" strokeLineCap="ROUND" strokeWidth="10.0" />
	<Arc fx:id="arc2" centerX="150.0" centerY="150.0" fill="#1f93ff00" length="85.0" radiusX="100.0" radiusY="100.0" startAngle="180.0" stroke="#3bff00" strokeWidth="10.0" />
	<Arc fx:id="arc1" centerX="150.0" centerY="150.0" fill="#1f93ff00" length="85.0" radiusX="100.0" radiusY="100.0" startAngle="90.0" stroke="#0f00ff" strokeWidth="10.0" />
	<Arc fx:id="arc4" centerX="150.0" centerY="150.0" fill="#1f93ff00" length="85.0" radiusX="100.0" radiusY="100.0" stroke="RED" strokeWidth="10.0" />
	</Group>
	<Button text="Go back" fx:id="backButton" onAction="#backClicked"/>

</VBox>
