<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="400.0" prefWidth="600.0" spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
      fx:controller="gamecode.SettingsController" alignment="CENTER" stylesheets="@/stylesheets/generalStyle.css, @/stylesheets/settingsStyle.css">

	<Label text="SETTINGS" fx:id="title" />
	<Button text="Change Orb" fx:id="orbButton" />
	<Button text="Pause music" fx:id="musicButton" onMouseClicked="#musicClicked"/>
	<Button text="Theme" fx:id="themeButton" onMouseClicked="#themeClicked"/>
	<Button text="Back" fx:id="backButton" onMouseClicked="#backClicked"/>


</VBox>
