<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="gamecode.GameOverController"
	  prefHeight="500.0" prefWidth="200.0" alignment="CENTER" spacing="20" stylesheets="@/stylesheets/generalStyle.css, @/stylesheets/gameOverStyle.css">

	<Label text="GAME OVER" fx:id="pauseMsg" />

<!--	<Button text="Resume" fx:id="resumeButton" onMouseClicked="#resumeClicked" />-->
<!--	<Button text="Save and quit" fx:id="saveButton" onMouseClicked="#saveClicked" />-->

	<HBox alignment="CENTER" spacing="20">
		<ImageView fx:id="backIcon" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#backClicked">
			<Image url="@../assets/back.png" />
		</ImageView>
		<ImageView fx:id="replayIcon" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" onMouseClicked="#replayClicked">
			<Image url="@../assets/replay3.png" />
		</ImageView>
	</HBox>



</VBox>
